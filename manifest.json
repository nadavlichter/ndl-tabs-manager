{
  "manifest_version": 3,
  "name": "TabCur - Advanced Tab Manager",
  "version": "1.0.0",
  "description": "An elegant, Apple-inspired tab manager with smart grouping and navigation features",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "navigate-to-previous-tab": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Navigate to previously active tab"
    }
  }
}